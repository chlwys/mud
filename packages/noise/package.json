    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node-fetch": "^2.6.2",
    "abdk-libraries-solidity": "^3.0.0",
    "assemblyscript": "^0.21.2",
    "base64-sol": "^1.1.0",
    "big-integer": "^1.6.51",
    "chai": "^4.3.6",
    "ethers": "^5.7.0",
