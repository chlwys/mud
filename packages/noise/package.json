  "main": "build/release.mjs",
  "types": "build/release.d.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn asbuild:release",
    "test:forge": "forge test",
    "test:hardhat": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' yarn hardhat test",
    "test": "yarn test:forge && yarn test:hardhat",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release && cp build/release.js build/release.mjs",
    "asbuild": "yarn asbuild:debug && yarn asbuild:release",
    "start": "npx serve .",
    "link": "yarn link"
  },
